{
  "name": "commercetools",
  "version": "1.0.0",
  "private": true,
  "main": "src/",
  "dependencies": {
    "@commercetools/api-request-builder": "^5.4.0",
    "@commercetools/sdk-client": "^2.1.1",
    "@commercetools/sdk-middleware-auth": "^6.0.1",
